#: src/window.ui:12
msgid "Desktop Files Creator"
msgstr "Desktop Files Creator"

#: data/com.github.alexkdeveloper.desktop-files-creator.desktop.in:4
msgid "Simple app for creating desktop files"
msgstr "Простое приложение для создания .desktop-файлов"

#: data/com.github.alexkdeveloper.desktop-files-creator.desktop.in:11
msgid "Desktop;File;Creator;"
msgstr "Desktop;File;Creator;рабочий стол;файл;"

#: data/com.github.alexkdeveloper.desktop-files-creator.appdata.xml.in:7
msgid "Simple app for creating desktop files on GNU/Linux"
msgstr "Простое приложение для создания .desktop-файлов в GNU/Linux"

#: data/com.github.alexkdeveloper.desktop-files-creator.appdata.xml.in:10
msgid ""
"The application allows you to create desktop files in GNU/Linux "
"distributions."
msgstr ""
"Приложение позволяет создавать .desktop-файлы в дистрибутивах GNU/Linux."

#: src/window.ui:20
msgid "Open"
msgstr "Открыть"

#: src/window.ui:21
msgid "Open Desktop Files Folder"
msgstr "Открыть папку с файлами"

#: src/window.ui:31
msgid "Create"
msgstr "Создать"

#: src/window.ui:32
msgid "Create Desktop File"
msgstr "Создать .desktop файл"

#: src/window.ui:56
msgid "Name"
msgstr "Название"

#: src/window.ui:72
msgid "Exec"
msgstr "Исполняемый файл"

#: src/window.ui:97
msgid "Icon"
msgstr "Значок приложения"

#: src/window.ui:122
msgid "Categories"
msgstr "Категории"

#: src/window.ui:138
msgid "Comment"
msgstr "Комментарий"

#: src/window.ui:154
msgid "Not Displayed in Menu"
msgstr "Не отображать в меню"

#: src/window.ui:168
msgid "Run in Terminal"
msgstr "Запускать в терминале"

#: src/window.vala:98
#, c-format
msgid ""
"Error!\n"
"Path: %s does not exist!\n"
"The program will not be able to perform its functions."
msgstr ""
"Ошибка!\n"
"Путь: %s не существует!\n"
"Программа не сможет выполнять свои функции."

#: src/window.vala:133
msgid "Open File"
msgstr "Открыть файл"

#: src/window.vala:158
msgid "Open Image"
msgstr "Открыть изображение"

#: src/window.vala:181
msgid "Enter the name"
msgstr "Введите название"

#: src/window.vala:187
msgid "A file with the same name already exists"
msgstr "Файл с таким названием уже существует"

#: src/window.vala:191
#, c-format
msgid "Create file %s?"
msgstr "Создать файл %s?"

#: src/window.vala:192
msgid "_Cancel"
msgstr "_Отменить"

#: src/window.vala:193 src/window.vala:258
msgid "_OK"
msgstr "_OK"

#: src/window.vala:241
#, c-format
msgid ""
"File %s is created!\n"
"Path: %s"
msgstr ""
"Файл %s был создан!\n"
"Путь: %s"

#: src/window.vala:243
msgid "Error! Could not create file"
msgstr "Ошибка! Не получилось создать файл"

